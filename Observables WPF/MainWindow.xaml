<Window x:Class="Observables_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Observables_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button Name="Start" Content="Start" Click="StartClick"/>
        <TextBox Name="ItemList" IsReadOnly="True" Height="250" VerticalScrollBarVisibility="Auto" IsReadOnlyCaretVisible="True"/>
    </StackPanel>
</Window>
